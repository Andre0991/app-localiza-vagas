<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    android:background="@drawable/back"
    tools:context="com.example.andreperictavares.projetocompartilhamentovagasdispmoveis.Activities.MainMenuActivity">

    <TextView
        android:text="O que deseja fazer?"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:textColor="#D3D3D3"
        android:fontFamily="serif"
        android:textSize="30sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text=""
        android:id="@+id/wellcome"
        android:layout_below="@+id/calcadaHorario"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Cadastrar calçada"
        android:layout_width="273dp"
        android:onClick="registerParkingLocation"
        android:background="@drawable/add"
        android:layout_height="100dp"
        android:textAlignment="textStart"
        android:gravity="start"
        android:id="@+id/registerParkingLocation"
        android:textColor="#000000"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:elevation="0dp" />

    <Button
        android:text="Cadastrar horário \n de calçada"
        android:layout_width="273dp"
        android:layout_height="100dp"
        android:onClick="goToCalcadaHorarioActivity"
        android:background="@drawable/data"
        android:id="@+id/calcadaHorario"
        android:textColor="#000000"
        android:textAlignment="textStart"
        android:gravity="start"
        android:layout_marginTop="47dp"
        android:layout_below="@+id/registerParkingLocation"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="@string/procurar_calcada_livre"
        android:layout_width="288dp"
        android:layout_height="104dp"
        android:background="@drawable/findparking03"
        android:textAlignment="textStart"
        android:gravity="start"
        android:onClick="goToSearchFreeParkingLocationActivity"
        android:id="@+id/searchAvailableParkingLocation"
        android:textColor="#000000"
        android:layout_above="@+id/registerParkingLocation"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="40dp" />


    </RelativeLayout>
